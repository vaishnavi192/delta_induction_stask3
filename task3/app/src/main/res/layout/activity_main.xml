<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".MainActivity2">

  <TextView
      android:id="@+id/textView4"
      android:layout_width="118dp"
      android:layout_height="39dp"
      android:layout_marginStart="9dp"
      android:layout_marginTop="21dp"
      android:layout_marginEnd="50dp"
      android:layout_marginBottom="330dp"
      android:text="You Owe"
      android:textColor="#FFFFFF"
      android:textSize="25sp"
      android:translationY="60sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/textView3"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/editView2"
      app:layout_constraintVertical_bias="1.0" />

  <TextView
      android:id="@+id/editView"
      android:layout_width="115dp"
      android:layout_height="53dp"
      android:layout_marginStart="78dp"
      android:layout_marginTop="33dp"
      android:layout_marginEnd="58dp"
      android:layout_marginBottom="390dp"
      android:text="535/-"
      android:textAlignment="center"
      android:textColor="#00FF0B"
      android:textSize="35sp"
      android:translationY="70sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/editView2"
      app:layout_constraintTop_toBottomOf="@+id/imageView"
      app:layout_constraintVertical_bias="1.0" />

  <View
      android:id="@+id/divider"
      android:layout_width="409dp"
      android:layout_height="3dp"
      android:layout_marginStart="1dp"
      android:layout_marginTop="75dp"
      android:layout_marginEnd="1dp"
      android:layout_marginBottom="655dp"
      android:background="@color/white"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0" />

  <TextView
      android:id="@+id/textView"
      android:layout_width="353dp"
      android:layout_height="55dp"
      android:text="Dashboard"
      android:textAlignment="center"
      android:textColor="#FFFFFF"
      android:textSize="45sp"
      tools:layout_editor_absoluteX="29dp"
      tools:layout_editor_absoluteY="6dp" />

  <ImageView
      android:id="@+id/imageView"
      android:layout_width="158dp"
      android:layout_height="130dp"
      android:layout_marginTop="47dp"
      android:layout_marginBottom="476dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/divider"
      app:srcCompat="@drawable/profile" />

  <TextView
      android:id="@+id/editView2"
      android:layout_width="115dp"
      android:layout_height="53dp"
      android:layout_marginStart="44dp"
      android:layout_marginTop="33dp"
      android:layout_marginEnd="79dp"
      android:layout_marginBottom="390dp"
      android:text="400/-"
      android:textAlignment="center"
      android:textColor="#FF0600"
      android:textSize="35sp"
      android:translationY="70sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/editView"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView"
      app:layout_constraintVertical_bias="1.0" />

  <TextView
      android:id="@+id/textView3"
      android:layout_width="118dp"
      android:layout_height="39dp"
      android:layout_marginStart="80dp"
      android:layout_marginTop="21dp"
      android:layout_marginEnd="8dp"
      android:layout_marginBottom="330dp"
      android:text="You Lent"
      android:textColor="#FFFFFF"
      android:textSize="25sp"
      android:translationY="60sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/textView4"
      app:layout_constraintTop_toBottomOf="@+id/editView"
      app:layout_constraintVertical_bias="1.0" />

  <TextView
      android:id="@+id/textView5"
      android:layout_width="290dp"
      android:layout_height="60dp"
      android:layout_marginStart="61dp"
      android:layout_marginTop="21dp"
      android:layout_marginEnd="61dp"
      android:text="Hello! VAISHNAVI"
      android:textAlignment="center"
      android:textColor="#FFFFFF"
      android:textSize="30sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView" />

  <Button
      android:id="@+id/button2"
      android:layout_width="139dp"
      android:layout_height="131dp"
      android:layout_marginTop="69dp"
      android:layout_marginBottom="-20dp"
      android:backgroundTint="#7D83D9"
      android:text="Equal Split"
      android:textSize="20sp"
      android:translationX="20sp"
      android:translationY="-10sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView4"
      app:layout_constraintVertical_bias="0.595" />

  <Button
      android:id="@+id/button3"
      android:layout_width="139dp"
      android:layout_height="131dp"
      android:layout_marginTop="150dp"
      android:layout_marginEnd="28dp"
      android:layout_marginBottom="130dp"
      android:backgroundTint="#7D83D9"
      android:text="Transactions"
      android:textSize="15sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView3"
      app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>